{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b0",
                    "template": " {{ .UserName }} ",
                    "foreground": "#FFFFFF",
                    "background": "#264653",
                    "type": "session",
                    "style": "diamond"
                },
                {
                    "properties": {
                        "cache_duration": "none",
                        "root_icon": "\uf292"
                    },
                    "template": " \uf0e7 ",
                    "foreground": "#FFFFFF",
                    "powerline_symbol": "\ue0b0",
                    "background": "#e76f51",
                    "type": "root",
                    "style": "powerline"
                },
                {
                    "properties": {
                        "cache_duration": "none",
                        "style": "folder"
                    },
                    "template": " {{ .Path }} ",
                    "foreground": "#FFFFFF",
                    "powerline_symbol": "\ue0b0",
                    "background": "#2a9d8f",
                    "type": "path",
                    "style": "powerline"
                },
                {
                    "properties": {
                        "cache_duration": "none",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
                    "foreground": "#000000",
                    "powerline_symbol": "\ue0b0",
                    "background": "#e9c46a",
                    "type": "git",
                    "style": "powerline",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#f4a261{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#e76f51{{ end }}",
                        "{{ if gt .Ahead 0 }}#2a9d8f{{ end }}",
                        "{{ if gt .Behind 0 }}#264653{{ end }}"
                    ]
                },
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": " \uf898 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
                    "foreground": "#000000",
                    "powerline_symbol": "\ue0b0",
                    "background": "#f4a261",
                    "type": "node",
                    "style": "powerline"
                },
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": " \ue626 {{ .Full }} ",
                    "foreground": "#FFFFFF",
                    "powerline_symbol": "\ue0b0",
                    "background": "#264653",
                    "type": "go",
                    "style": "powerline"
                },
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": " \ue624 {{ .Full }} ",
                    "foreground": "#FFFFFF",
                    "powerline_symbol": "\ue0b0",
                    "background": "#2a9d8f",
                    "type": "python",
                    "style": "powerline"
                },
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": " \uf0e7 ",
                    "foreground": "#FFFFFF",
                    "powerline_symbol": "\ue0b0",
                    "background": "#e76f51",
                    "type": "shell",
                    "style": "powerline"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "properties": {
                        "cache_duration": "none",
                        "time_format": "15:04:05"
                    },
                    "leading_diamond": "\ue0b2",
                    "trailing_diamond": "\ue0b4",
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "foreground": "#FFFFFF",
                    "background": "#264653",
                    "type": "time",
                    "style": "diamond"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "properties": {
                        "cache_duration": "none"
                    },
                    "template": "‚ùØ ",
                    "foreground": "#2a9d8f",
                    "type": "text",
                    "style": "plain"
                }
            ],
            "newline": true
        }
    ],
    "version": 3
}